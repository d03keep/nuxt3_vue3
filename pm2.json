{
  "apps": [
    {
      "name": "nuxt3_app",
      "script": "./.output/server/index.mjs",
      "env": {
        "NODE_ENV": "production"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "instances": 4,
      "exec_mode": "cluster",
      "max_restarts" : 10,
      "restart_delay" : 3000,
      "log_date_format" : "YYYY-MM-DD HH:mm Z",
      "combine_logs" : true,
      "log_file": "./.output/public/log/nuxt3.log",
      "out_file": "./.output/public/log/nuxt3.out.log",
      "error_file": "./.output/public/log/nuxt3.error.log",
      "watch": true
    }
  ]
}
